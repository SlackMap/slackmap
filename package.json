    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "dev:ssr": "ng run web:serve-ssr",
    "serve:ssr": "node -r source-map-support/register dist/apps/web/server/main.js",
    "build:ssr": "ng build web --prod && ng run web:server:production",
    "bs:ssr": "npm run build:ssr && npm run copy-assets:next && npm run serve:ssr",
    "serve:pwa": "http-server -p 8080 -c-1 dist/apps/web/browser",
    "serve:api": "node dist/apps/api/main.js",
    "prerender": "ng run web:prerender",
    "build:all": "ng build api --prod && npm run build:ssr",
    "bump:next": "standard-version --prerelease next -t '' --skip.tag=true",
    "bump:prod": "standard-version --no-verify -t '' --message 'chore(release): %s [skip ci]'",
    "copy-assets:next": "node ./tools/scripts/copy-assets.js",
    "copy-assets:prod": "node ./tools/scripts/copy-assets.js",
    "build:next": "npm run build:all && npm run copy-assets:next",
    "build:prod": "npm run build:all && npm run copy-assets:prod",
    "deploy:next": "npm run bump:next && npm run copy-assets:next && shipit next deploy",
    "deploy:prod": "npm run bump:prod && npm run copy-assets:prod && shipit prod deploy",
    "rollback:next": "shipit next rollback",
    "rollback:prod": "shipit prod rollback",
    "bd:next": "npm run build:next && npm run deploy:next",
    "bd:prod": "npm run build:prod && npm run deploy:prod"
  },
