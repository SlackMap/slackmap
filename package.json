    "install:dev": "npm i @liberation-data/drivine@dev --registry http://localhost:4873",
    "neo4j": "docker run --env NEO4J_AUTH=neo4j/root --name=slackmap --publish=7474:7474 --publish=7687:7687 --volume=C:/Users/Piotr/www/slackmap/storage/neo4j/data:/data --volume=C:/Users/Piotr/www/slackmap/storage/neo4j/logs:/logs neo4j:4.0",
