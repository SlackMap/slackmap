{
  "name": "slackmap",
  "version": "0.1.1-next.0",
  "license": "MIT",
  "bin": {
    "sm": "./apps/cli/src/sm"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "dev:ssr": "ng run web:serve-ssr",
    "serve:ssr": "node dist/apps/web/server/main.js",
    "build:ssr": "ng build web --prod && ng run web:server:production",
    "prerender": "ng run web:prerender",
    "serve:pwa": "http-server -p 8080 -c-1 dist/apps/web/browser",
    "db": "set ORIENTDB_ROOT_PASSWORD=root &&  bash ./db/current/bin/server.sh",
    "db:backup": "shipit --shipitfile shipitfile-db-backup.js",
    "db:deploy": "shipit --shipitfile shipitfile-db-deploy.js",
    "build:all": "ng build api --prod && npm run build:ssr",
    "bump:test": "standard-version --prerelease next -t ''",
    "bump:prod": "standard-version --no-verify -t '' --message 'chore(release): %s [skip ci]'",
    "copy-assets:test": "node ./tools/scripts/copy-assets.js",
    "copy-assets:prod": "node ./tools/scripts/copy-assets.js",
    "deploy:test": "shipit test deploy",
    "deploy:prod": "shipit prod deploy",
    "bd:test": "npm run build:all && npm run bump:test && npm run copy-assets:test && npm run deploy:test",
    "bd:prod": "npm run build:all && npm run bump:prod && npm run copy-assets:prod && npm run deploy:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/cdk": "^9.1.2",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/forms": "9.0.0",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/platform-server": "9.0.0",
    "@angular/router": "9.0.0",
    "@angular/service-worker": "9.0.0",
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/platform-express": "^6.8.3",
    "@nguniversal/express-engine": "^9.0.1",
    "chalk": "^3.0.0",
    "clear": "^0.1.0",
    "commander": "^5.0.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "figlet": "^1.3.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.1.0",
    "ora": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.1",
    "@angular/cli": "9.0.1",
    "@angular/compiler-cli": "9.0.0",
    "@angular/language-service": "9.0.0",
    "@babel/core": "7.8.7",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.3",
    "@nguniversal/builders": "^9.0.1",
    "@nrwl/angular": "^9.0.4",
    "@nrwl/cypress": "9.0.4",
    "@nrwl/jest": "9.0.4",
    "@nrwl/nest": "^9.0.4",
    "@nrwl/node": "9.0.4",
    "@nrwl/storybook": "*",
    "@nrwl/workspace": "9.0.4",
    "@storybook/addon-knobs": "5.3.9",
    "@storybook/angular": "5.3.9",
    "@storybook/core": "^5.3.17",
    "@storybook/react": "5.3.9",
    "@types/express": "^4.17.0",
    "@types/jest": "24.9.1",
    "@types/node": "~8.9.4",
    "babel-loader": "8.0.6",
    "codelyzer": "~5.0.1",
    "cypress": "^3.8.2",
    "dotenv": "^6.2.0",
    "eslint": "6.1.0",
    "http-server": "^0.12.1",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "prettier": "1.18.2",
    "shipit-cli": "^5.2.0",
    "shipit-deploy": "^5.2.0",
    "standard-version": "^7.1.0",
    "targz": "^1.0.1",
    "ts-jest": "24.0.0",
    "ts-node": "^8.6.2",
    "tslint": "~5.11.0",
    "typescript": "~3.7.4"
  }
}
