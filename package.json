  "name": "slackmap",
  "version": "0.5.0-next.4",
  "license": "MIT",
  "bin": {
    "sm": "./apps/cli/src/sm"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "jest": "jest --clearCache",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "migrations": "ts-node --project=libs/api/orient/tsconfig.migrations.json libs/api/orient/src/migrations/cli.ts",
    "build:migrations": "tsc -p libs/api/orient/tsconfig.migrations.json",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "dev:ssr": "ng run web:serve-ssr",
    "serve:ssr": "node -r source-map-support/register dist/apps/web/server/main.js",
    "build:ssr": "ng build web --prod && ng run web:server:production",
    "serve:pwa": "http-server -p 8080 -c-1 dist/apps/web/browser",
    "prerender": "ng run web:prerender",
    "db": "set ORIENTDB_ROOT_PASSWORD=root &&  bash ./db/current/bin/server.sh",
    "db:backup": "shipit --shipitfile shipitfile-db-backup.js",
    "db:deploy": "shipit --shipitfile shipitfile-db-deploy.js",
    "build:all": "ng build api --prod && npm run build:ssr",
    "bump:test": "standard-version --prerelease next -t '' --skip.tag=true",
    "bump:prod": "standard-version --no-verify -t '' --message 'chore(release): %s [skip ci]'",
    "copy-assets:test": "node ./tools/scripts/copy-assets.js",
    "copy-assets:prod": "node ./tools/scripts/copy-assets.js",
    "build:test": "npm run build:all && npm run bump:test && npm run copy-assets:test",
    "build:prod": "npm run build:all && npm run bump:prod && npm run copy-assets:prod",
    "deploy:test": "shipit test deploy",
    "deploy:prod": "shipit prod deploy",
    "bd:test": "npm run build:test && npm run deploy:test",
    "bd:prod": "npm run build:prod && npm run deploy:prod"
  },
  "private": true,
