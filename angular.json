          }
        }
      },
      "schematics": {}
    },
    "api-client": {
      "root": "libs/api-client",
      "sourceRoot": "libs/api-client/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
