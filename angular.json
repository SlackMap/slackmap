      }
    },
    "ui-api": {
      "projectType": "library",
      "root": "libs/ui/api",
      "sourceRoot": "libs/ui/api/src",
      "prefix": "slackmap",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
