          "options": {
            "tsConfig": [
              "apps/ui-pwa-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ui-pwa-e2e/**"
            ]
          }
        }
      }
    },
    "cli": {
      "root": "apps/cli",
      "sourceRoot": "apps/cli/src",
      "projectType": "application",
      "prefix": "cli",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/cli",
            "main": "apps/cli/src/sm.ts",
            "tsConfig": "apps/cli/tsconfig.app.json",
